{
  "expo": {
    "name": "GiveSpace",
    "slug": "givespace",
    "version": "2.1.0",
    "assetBundlePatterns": ["**/*"],
    "plugins": [
      "expo-contacts",
      "expo-screen-orientation",
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let customize your profile."
        }
      ],
      "expo-file-system"
    ],
    "packagerOpts": {
      "config": "metro.config.js",
      "sourceExts": [
        "expo.ts",
        "expo.tsx",
        "expo.js",
        "expo.jsx",
        "ts",
        "tsx",
        "js",
        "jsx",
        "json",
        "wasm",
        "svg"
      ]
    },
    "android": {
      "permissions": [
        "android.permission.READ_CONTACTS",
        "android.permission.WRITE_CONTACTS",
        "android.permission.CAMERA",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.RECORD_AUDIO",
        "android.permission.INTERNET"
      ],
      "package": "com.givespace"
    }
  },
  "name": "Project-Wishlist"
}
