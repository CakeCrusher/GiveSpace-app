{
  "expo": {
    "name": "GiveSpace",
    "slug": "givespace",
    "version": "1.2.0",
    "sdkVersion": "38.0.0",
    "icon": "./resources/img/icon.png",
    "assetBundlePatterns": ["**/*"],
    "plugins": [
      "expo-contacts",
      "expo-screen-orientation",
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let customize your profile."
        }
      ],
      "expo-file-system"
    ],
    "splash": {
      "image": "./resources/img/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "packagerOpts": {
      "config": "metro.config.js",
      "sourceExts": [
        "expo.ts",
        "expo.tsx",
        "expo.js",
        "expo.jsx",
        "ts",
        "tsx",
        "js",
        "jsx",
        "json",
        "wasm",
        "svg"
      ]
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#c9042c",
        "foregroundImage": "./resources/img/icon.png"
      },
      "permissions": [
        "android.permission.READ_CONTACTS",
        "android.permission.WRITE_CONTACTS",
        "android.permission.CAMERA",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.RECORD_AUDIO",
        "android.permission.INTERNET"
      ]
    }
  },
  "name": "GiveSpace"
}
